{
  "name": "gantries",
  "version": "0.0.1",
  "description": "See Singapore's ERP rates easily",
  "scripts": {
    "build": "react-scripts --max_old_space_size=4096 build",
    "eject": "react-scripts eject",
    "fetch:rates": "npx ts-node scripts/fetch-rates.ts",
    "gen:tileset-source": "npx ts-node scripts/generate-tileset-source.ts",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "start": "react-scripts start",
    "test:scripts": "jest --testPathPattern=scripts/.*.test.ts",
    "test": "react-scripts test",
    "tsc": "npx tsc --noEmit"
  },
  "keywords": [
    "erp",
    "electronic road pricing"
  ],
  "author": "Sebastian Quek",
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/mapbox-gl": "^2.6.1",
    "@types/node": "^17.0.18",
    "@types/papaparse": "^5.3.2",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.23",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "babel-jest": "^27.5.1",
    "dotenv": "^16.0.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "form-data": "^4.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "csv-stringify": "^6.0.5",
    "lodash": "^4.17.21",
    "mapbox-gl": "^2.7.0",
    "papaparse": "^5.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-scripts": "^5.0.0",
    "styled-components": "^5.3.3",
    "web-vitals": "^2.1.4"
  },
  "browserslist": {
    "production": [
      "defaults",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
